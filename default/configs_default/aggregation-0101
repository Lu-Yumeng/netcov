interface Loopback0
  ip address 10.1.3.0/32
interface Serial0
  ip address 70.0.6.0/31
  media-type gbic
  speed 1000
  duplex full
  no negotiation auto
  no cdp enable
interface Serial1
  ip address 70.0.7.0/31
  media-type gbic
  speed 1000
  duplex full
  no negotiation auto
  no cdp enable
interface Serial2
  ip address 70.0.25.1/31
  media-type gbic
  speed 1000
  duplex full
  no negotiation auto
  no cdp enable
interface Serial3
  ip address 70.0.29.1/31
  media-type gbic
  speed 1000
  duplex full
  no negotiation auto
  no cdp enable
router bgp 10009
  bgp bestpath as-path multipath-relax
  maximum-paths eibgp 4
  neighbor 70.0.6.1 remote-as 10010
  neighbor 70.0.6.1 send-community
  neighbor 70.0.7.1 remote-as 10011
  neighbor 70.0.7.1 send-community
  neighbor 70.0.25.0 remote-as 10002
  neighbor 70.0.25.0 send-community
  neighbor 70.0.29.0 remote-as 10003
  neighbor 70.0.29.0 send-community
end