interface Loopback0
  ip address 10.2.2.0/32
interface Serial0
  ip address 70.0.8.0/31
  media-type gbic
  speed 1000
  duplex full
  no negotiation auto
  no cdp enable
interface Serial1
  ip address 70.0.9.0/31
  media-type gbic
  speed 1000
  duplex full
  no negotiation auto
  no cdp enable
interface Serial2
  ip address 70.0.18.1/31
  media-type gbic
  speed 1000
  duplex full
  no negotiation auto
  no cdp enable
interface Serial3
  ip address 70.0.22.1/31
  media-type gbic
  speed 1000
  duplex full
  no negotiation auto
  no cdp enable
router bgp 10012
  bgp bestpath as-path multipath-relax
  maximum-paths eibgp 4
  neighbor 70.0.8.1 remote-as 10014
  neighbor 70.0.8.1 send-community
  neighbor 70.0.9.1 remote-as 10015
  neighbor 70.0.9.1 send-community
  neighbor 70.0.18.0 remote-as 10000
  neighbor 70.0.18.0 send-community
  neighbor 70.0.22.0 remote-as 10001
  neighbor 70.0.22.0 send-community
end