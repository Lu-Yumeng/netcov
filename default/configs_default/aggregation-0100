interface Loopback0
  ip address 10.1.2.0/32
interface Serial0
  ip address 70.0.4.0/31
  media-type gbic
  speed 1000
  duplex full
  no negotiation auto
  no cdp enable
interface Serial1
  ip address 70.0.5.0/31
  media-type gbic
  speed 1000
  duplex full
  no negotiation auto
  no cdp enable
interface Serial2
  ip address 70.0.17.1/31
  media-type gbic
  speed 1000
  duplex full
  no negotiation auto
  no cdp enable
interface Serial3
  ip address 70.0.21.1/31
  media-type gbic
  speed 1000
  duplex full
  no negotiation auto
  no cdp enable
router bgp 10008
  bgp bestpath as-path multipath-relax
  maximum-paths eibgp 4
  neighbor 70.0.4.1 remote-as 10010
  neighbor 70.0.4.1 send-community
  neighbor 70.0.5.1 remote-as 10011
  neighbor 70.0.5.1 send-community
  neighbor 70.0.17.0 remote-as 10000
  neighbor 70.0.17.0 send-community
  neighbor 70.0.21.0 remote-as 10001
  neighbor 70.0.21.0 send-community
end