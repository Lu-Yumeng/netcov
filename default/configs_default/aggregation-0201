interface Loopback0
  ip address 10.2.3.0/32
interface Serial0
  ip address 70.0.10.0/31
  media-type gbic
  speed 1000
  duplex full
  no negotiation auto
  no cdp enable
interface Serial1
  ip address 70.0.11.0/31
  media-type gbic
  speed 1000
  duplex full
  no negotiation auto
  no cdp enable
interface Serial2
  ip address 70.0.26.1/31
  media-type gbic
  speed 1000
  duplex full
  no negotiation auto
  no cdp enable
interface Serial3
  ip address 70.0.30.1/31
  media-type gbic
  speed 1000
  duplex full
  no negotiation auto
  no cdp enable
router bgp 10013
  bgp bestpath as-path multipath-relax
  maximum-paths eibgp 4
  neighbor 70.0.10.1 remote-as 10014
  neighbor 70.0.10.1 send-community
  neighbor 70.0.11.1 remote-as 10015
  neighbor 70.0.11.1 send-community
  neighbor 70.0.26.0 remote-as 10002
  neighbor 70.0.26.0 send-community
  neighbor 70.0.30.0 remote-as 10003
  neighbor 70.0.30.0 send-community
end